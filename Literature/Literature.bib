
@article{shibani_financial_2022,
	title = {Financial risk management in the construction projects},
	issn = {10183639},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S1018363922000435},
	doi = {10.1016/j.jksues.2022.05.001},
	abstract = {Construction risk can be described as the possibility of an incident that will harm the project's feasibility. Different threats that impact businesses may be found in the construction industry as elsewhere. Moreover, construction projects face significant risks, which put building projects at the risk of cost, over time, and poor standard of execution. However, in the last two years, Lebanon has been suffering from a severe economic crisis, which has affected various numbers of businesses and industries, specifically the construction industry. Therefore, this article is carried out to identify, classify and analyse the most significant risks inherent in the Lebanese construction industry with special emphasis on the financial and economic risk category and eventually establish a conclusion in this regard. To achieve the aims of this research, the data was collected by conducting a questionnaire survey among experts in the Lebanese construction industry. The results showed that the construction industry in Lebanon is exposed to many risk sources, internal and external and the most important are the financial risk such as fluctuation of the currency, inflation, lack of solvency, etc. Finally, the importance and benefits of implementing risk management are discussed in this research as well as the barriers to its effective implementation.},
	language = {en},
	urldate = {2023-09-14},
	journal = {Journal of King Saud University - Engineering Sciences},
	author = {Shibani, Abdussalam and Hasan, Dyaa and Saaifan, Jalal and Sabboubeh, Heba and Eltaip, Mohamad and Saidani, Messaoud and Gherbal, Nawal},
	month = may,
	year = {2022},
	pages = {S1018363922000435},
	annote = {The article titled "Financial risk management in the construction projects" by Abdussalam Shibani et al. focuses on identifying, classifying, and analyzing significant risks in the Lebanese construction industry, with a special emphasis on financial and economic risks. The article is particularly timely given the severe economic crisis Lebanon has been facing, which has had a considerable impact on the construction industry.
Key Points:


Introduction and Context: The article starts by highlighting the importance of risk management in construction projects and the unique challenges faced by the Lebanese construction industry due to the country's ongoing economic crisis (Page 1).


Risk Classification: The article categorizes risks into internal and external types. Internal risks are related to team management and include responsibilities of owners, architects, suppliers, designers, contractors, and sub-contractors. External risks are broader and include political, socio-cultural, and economic factors. The article pays special attention to economic risks like inflation, currency fluctuation, and financial challenges due to the economic crisis in Lebanon (Pages 2-3).


Pandemic Risk: The article also discusses the impact of pandemics like COVID-19 on the construction industry, affecting the supply chain and leading to project delays (Page 3).


Benefits of Implementing Risk Management: The article concludes by emphasizing the benefits of effective risk management, such as increased efficiency, better project completion rates, and a more systematic approach to risk assessment (Page 3).


Relevance to Your Research Question:
Your research question aims to integrate machine learning models for accurately forecasting margins and risks in construction projects. This article provides valuable insights into the types of risks, especially financial and economic ones, that are prevalent in the construction industry. Understanding these risks could be crucial for your machine learning model to make accurate forecasts and risk assessments. The article's focus on the Lebanese context, which is undergoing an economic crisis, could also offer a challenging scenario for testing the robustness of your predictive model.
},
	file = {Accepted Version:/Users/tobiasbrammer/Zotero/storage/RJQQU9PI/Shibani et al. - 2022 - Financial risk management in the construction proj.pdf:application/pdf},
}

@article{serpella_risk_2014,
	title = {Risk {Management} in {Construction} {Projects}: {A} {Knowledge}-based {Approach}},
	volume = {119},
	issn = {18770428},
	shorttitle = {Risk {Management} in {Construction} {Projects}},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S1877042814021648},
	doi = {10.1016/j.sbspro.2014.03.073},
	abstract = {One of the major roles undertaken by a project manager is the management of the risk of a project. However, this duty is particularly complex and inefficient if good risk management has not been done from the beginning of the project. An effective and efficient risk management approach requires a proper and systematic methodology and, more importantly, knowledge and experience. Previous research results in Chile have shown that both, owners and contractors do not systematically apply risk management practices, resulting in negative consequences for projects’ performance. This paper addresses the problems of risk management in construction projects using a knowledge-based approach, and proposes a methodology based on a three-fold arrangement that includes the modeling of the risk management function, its evaluation, and the availability of a best practices model. This approach is part of a * research effort that is underway. A major preliminary conclusion of this research is the fact that risk management in construction projects is still very ineffective and that the main cause of this situation is the lack of knowledge. It is expected that the application of the proposed approach will allow clients and contractors to develop a project's risk management function based on best practices, and also to improve the performance of this function.},
	language = {en},
	urldate = {2023-09-14},
	journal = {Procedia - Social and Behavioral Sciences},
	author = {Serpella, Alfredo Federico and Ferrada, Ximena and Howard, Rodolfo and Rubio, Larissa},
	month = mar,
	year = {2014},
	pages = {653--662},
	annote = {The paper titled "Risk management in construction projects: a knowledge-based approach" by Alfredo Federico Serpella et al. delves into the complexities and inefficiencies of risk management in construction projects. The paper proposes a methodology based on a three-fold arrangement that includes modeling the risk management function, its evaluation, and the availability of a best practices model.
Key Points:


Introduction and Problem Statement: The paper identifies that risk management in construction projects is often ineffective due to a lack of systematic application and knowledge. It argues that both owners and contractors do not apply risk management practices systematically, leading to negative consequences for project performance (Pages 1-2).


Knowledge-Based Approach: The paper proposes a knowledge-based approach to risk management in construction projects. This approach aims to develop a risk management function based on best practices and to improve the performance of this function (Page 2).


Background on Risk Management: The paper discusses the importance of risk management as an integral part of project management. It outlines the main steps in the risk management process, including risk planning, identification, assessment, analysis, response, and monitoring (Page 3).


Maturity Models: The paper discusses the use of maturity models as a tool to assess the risk management capability of an organization. It reviews several known risk management maturity models and their attributes (Page 4).


Knowledge Management: The paper argues that managing the information and knowledge of a construction project is an essential part of a successful project risk management initiative. It suggests that a knowledge management approach could improve the deficiencies of the risk management process (Page 5).


Relevance to Your Research Question:
Your research question focuses on integrating machine learning models for accurately forecasting margins and risks in construction projects. This paper is highly relevant as it addresses the inefficiencies and complexities in risk management in construction projects. The knowledge-based approach proposed in the paper could serve as a foundational layer upon which machine learning models could be built for better risk assessment and forecasting. The discussion on maturity models could also be useful for evaluating the effectiveness of your proposed machine learning models over time.
},
	file = {Serpella et al. - 2014 - Risk Management in Construction Projects A Knowle.pdf:/Users/tobiasbrammer/Zotero/storage/59RZU98E/Serpella et al. - 2014 - Risk Management in Construction Projects A Knowle.pdf:application/pdf},
}

@article{srinivasan_factors_2022,
	title = {Factors influencing financial risk management in construction projects},
	volume = {1125},
	issn = {1755-1307, 1755-1315},
	url = {https://iopscience.iop.org/article/10.1088/1755-1315/1125/1/012025},
	doi = {10.1088/1755-1315/1125/1/012025},
	abstract = {Abstract
            The infrastructural and economic development of a country merely depends on the construction industry and its growth. There are many risk factors involved with the construction industry during planning, execution and commissioning of the projects. Dealing with the risk factors in construction industry have been known as a crucial management process in order to achieve the overall objectives of the industry in terms of cost, time, safety, quality and environmental sustainability. This report is related with the financial aspects which directly take part in the industry for which studies have been conducted with various engineers, contractors and labors in and around north-western districts of Tamilnadu. The perspectives at different levels of management have been concluded with the standard questionnaire format that has been investigated and their results are analyzed based on the current scenario. With the help of these surveys and data analysis by Relative Importance Index (RII), the most related factors among the identified factors were found to be labour risk, material risk, credit risk, planning risk, execution and environmental related factors. The suggestions and recommendations have been expressed to overcome those risk moderations.},
	number = {1},
	urldate = {2023-09-14},
	journal = {IOP Conf. Ser.: Earth Environ. Sci.},
	author = {Srinivasan, N P and Dinesh, A and Munshi, Surajit and Karthick, A},
	month = dec,
	year = {2022},
	pages = {012025},
	annote = {The article titled "Factors influencing financial risk management in construction projects" by N P Srinivasan et al. delves into the financial risks associated with the construction industry, particularly in the north-western districts of Tamilnadu, India. The paper aims to identify and analyze various risk factors that can impact the financial stability of construction projects.
Key Points:


Introduction and Objectives: The article emphasizes the importance of managing financial risks in the construction industry. It outlines that the industry's growth is closely tied to a country's infrastructural and economic development. The paper aims to identify the major risks leading to financial instability in construction projects (Page 2).


Methodology: The study employs a questionnaire survey involving various stakeholders like engineers, contractors, and laborers. The data collected is analyzed using the Relative Importance Index (RII) to rank the identified risk factors (Page 3).


Identified Risk Factors: The paper lists several risk factors related to financial management in construction projects, such as labor risk, material risk, credit risk, planning risk, execution risk, and environmental risk. These factors are further analyzed to understand their impact (Pages 2-3).


Data Analysis and Results: The RII method shows that labor risk has the highest impact on financial management, followed by material risk, credit risk, execution risk, and environmental risk. The paper suggests that labor availability and material risks need to be closely monitored for effective financial management (Page 4).


Conclusion: The paper concludes by recommending proper inventory management, well-established credit details, and a structured hierarchy for task execution as ways to mitigate financial risks in construction projects (Page 5).


Relevance to Your Research Question:
Your research question focuses on using integrated machine learning models to forecast margins and risks in construction projects. This article provides a comprehensive list of financial risk factors that could be incorporated into your machine learning model as features. The methodology used in the article, particularly the Relative Importance Index (RII), could also be adapted in your research for ranking the importance of various risk factors. The focus on labor and material risks aligns well with your aim to create a comprehensive approach that integrates forecasting, risk assessment, and actionable recommendations.
},
	file = {Full Text:/Users/tobiasbrammer/Zotero/storage/Y2JG372N/Srinivasan et al. - 2022 - Factors influencing financial risk management in c.pdf:application/pdf},
}

@article{dziadosz_financial_2015,
	title = {Financial {Risk} {Estimation} in {Construction} {Contracts}},
	volume = {122},
	issn = {18777058},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S1877705815031045},
	doi = {10.1016/j.proeng.2015.10.015},
	abstract = {Risk, as a factor adversely affecting the project, is taken into account as early as at the first stage of the investment process when the tender is won for a contract. The risk level, identified for a particular construction contract, is a serious factor influencing the decision about accepting the contract or not. The key issue is proper identification of contract risk. The risk factors which have a significant impact on the success of the project, and are most common ones, are then analyzed. The method of verification depends on the company's experience in the construction industry. The task of the article is to present the results of studies focused on separating and determining the frequency of financial risk factors in construction projects and their impact on project implementation. We have analyzed 30 construction projects (office buildings, production halls, educational buildings, demolition works, etc.) completed in north-western Poland. The degree of risk (planned and real) for the contract was set as a percentage in relation to the size of contract (costs in million PLN). The aim of the study was to verify the existence of relationship between the kind of structure, the size of contract, and the scope and degree of risk. Statistical approach has been used. This study is a prelude to determining the contract risks identification procedure (i.e., estimating reserves to be used in unforeseen circumstances) so that the company could be competitive, and the price offer to be provided to investors could be advantageous.},
	language = {en},
	urldate = {2023-09-15},
	journal = {Procedia Engineering},
	author = {Dziadosz, Agnieszka and Tomczyk, Andrzej and Kapliński, Oleg},
	year = {2015},
	pages = {120--128},
	annote = {The paper titled "Financial risk estimation in construction contracts" by Agnieszka Dziadosz, Andrzej Tomczyk, and Oleg Kapliński focuses on identifying and analyzing financial risk factors in construction projects. The paper aims to provide insights into the relationship between the type of structure, the size of the contract, and the scope and degree of risk.
Key Points:


Objective: The paper aims to identify financial risk factors in construction projects and their impact on project implementation. It also seeks to develop a financial risk analysis model for assessing construction contracts (Page 1).


Methodology: The study analyzed 30 construction projects in north-western Poland, focusing on office buildings, production halls, educational buildings, and demolition works. A statistical approach was used to verify the existence of relationships between the kind of structure, the size of the contract, and the scope and degree of risk (Page 1).


Types of Contracts: The paper discusses different types of contracts in Poland, such as public procurement and private orders. It emphasizes that the method of risk evaluation depends on the company's experience in the construction industry (Page 2).


Risk Categories: The paper identifies various types of risks, including financial, technological, economic, organizational, management, time-span, and legal risks. It aims to identify the factors with the biggest impact on the implementation of the contract (Page 3).


Results: The study found that the average value of real financial risk amounts to 3.77\% in relation to the contract value. It also found that there is a weak relationship between the size of the contract and the degree of financial risk (Page 4-5).


Relevance to Your Research Question:
Your research question focuses on the integration of machine learning models for accurately forecasting margins and risks in construction projects. This paper is relevant in several ways:


Financial Risk Factors: The paper's focus on identifying and analyzing financial risk factors can provide valuable data points for your machine learning models.


Statistical Approach: The paper's statistical methodology for analyzing the relationship between various factors and risks could offer a methodological framework that you might integrate into your machine learning models.


Types of Contracts and Risks: Understanding the different types of contracts and associated risks can help you tailor your machine learning models to specific scenarios, thereby improving their predictive accuracy.


Risk Categories: The paper's comprehensive categorization of risks can serve as a useful framework for your research, allowing you to consider multiple dimensions of risk in construction projects.


},
	file = {Dziadosz et al. - 2015 - Financial Risk Estimation in Construction Contract.pdf:/Users/tobiasbrammer/Zotero/storage/VMCZ7CTC/Dziadosz et al. - 2015 - Financial Risk Estimation in Construction Contract.pdf:application/pdf},
}

@article{blair_forecasting_1993,
	title = {Forecasting construction cost escalation},
	volume = {20},
	issn = {0315-1468, 1208-6029},
	url = {http://www.nrcresearchpress.com/doi/10.1139/l93-077},
	doi = {10.1139/l93-077},
	abstract = {Escalation can account for a substantial part of construction costs. Therefore forecasts of the amount of escalation are required for budgetary and bidding purposes. This paper examines methods for forecasting construction escalation using statistical time series methods. Time series of construction cost indices are used as a proxy of construction cost escalation. The application of time series methods, their limitations, and their effect on the risk of cost escalation are demonstrated and evaluated. The analytical methods available are only useful in forecasting for short construction projects in stable conditions. This is because none of the methods can forecast escalation caused by unpredictable occurrences such as outbreak of war or certain government action. Construction cost escalation remains a risk to be borne by either the contractor or the owner, or both, depending on the terms of the contract; any logical approach to minimize the risk is worthwhile. Key words: construction cost escalation, cost indices, time series forecasting, exponential smoothing. Box–Jenkins methods, dynamic regression, Statistics Canada.},
	language = {en},
	number = {4},
	urldate = {2023-09-15},
	journal = {Can. J. Civ. Eng.},
	author = {Blair, Andrew N. and Lye, Leonard M. and Campbell, W. J.},
	month = aug,
	year = {1993},
	pages = {602--612},
	annote = {The paper titled "Forecasting construction cost escalation" by Andrew N. Blair, Leonard M. Lye, and W.J. Campbell focuses on the methods for forecasting construction cost escalation using statistical time series methods. The paper examines the application of these methods, their limitations, and their effects on the risk of cost escalation.
Key Points:


Objective: The paper aims to explore methods for forecasting construction cost escalation, which is a significant part of construction costs. The focus is on using statistical time series methods for this purpose (Page 2).


Methodology: The paper uses time series of construction cost indices as a proxy for construction cost escalation. It discusses various forecasting methods, including exponential smoothing and Box-Jenkins methods (Page 2).


Limitations: The paper acknowledges that the analytical methods are useful only for short construction projects in stable conditions. They cannot forecast escalation caused by unpredictable events like wars or government actions (Page 2).


Forecasting Methods: The paper categorizes forecasting methods into three types: subjective methods, univariate methods, and multivariate methods. It delves into the benefits and limitations of each (Pages 3-5).


Risk Assessment: The paper concludes that construction cost escalation remains a risk to be borne by either the contractor or the owner, depending on the contract terms. It suggests that any logical approach to minimize this risk is worthwhile (Page 2).


Relevance to Your Research Question:
Your research question focuses on the use of integrated machine learning models for accurately forecasting margins and risks in construction projects. This paper is relevant in several ways:


Methodological Insights: The paper's focus on statistical time series methods for forecasting construction cost escalation could offer additional techniques that you might integrate into your machine learning models.


Risk Assessment: The paper emphasizes the risk of cost escalation in construction projects, which aligns with your research aim to offer a robust tool for decision-makers in construction project management.


Limitations of Current Methods: Understanding the limitations of existing forecasting methods, such as their inability to predict escalation due to unpredictable events, could help you in developing a more robust machine learning model.


Data Types: The use of construction cost indices as a proxy for cost escalation could be a useful feature in your predictive model.


},
	file = {Blair et al. - 1993 - Forecasting construction cost escalation.pdf:/Users/tobiasbrammer/Zotero/storage/9NBKUIX4/Blair et al. - 1993 - Forecasting construction cost escalation.pdf:application/pdf},
}

