@article{kucharavy_application_2011,
	title = {Application of {S}-shaped curves},
	volume = {9},
	issn = {18777058},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S1877705811001597},
	doi = {10.1016/j.proeng.2011.03.142},
	abstract = {This paper deals with the application of S-shaped curves in the contexts of inventive problem solving, innovation and technology forecasts. After explaining the origin of the logistic S-curve its application, as seen in publications from different domains, is reviewed. Despite much criticism and the failure to apply the logistic function for long-term forecasting, it continues to be a popular model for describing the evolution of systems (technological, economical, social and others) over time. This paper can help researchers and practitioners to understand the scope of application of S-shaped patterns, their limitations, and peculiarities.},
	language = {en},
	urldate = {2023-09-15},
	journal = {Procedia Engineering},
	author = {Kucharavy, Dmitry and De Guio, Roland},
	year = {2011},
	pages = {559--572},
	annote = {The paper titled "Application of S-Shaped Curves" by Dmitry Kucharavy and Roland De Guio explores the utility of S-shaped curves in various contexts, including inventive problem-solving, innovation, and technology forecasts. The paper delves into the history, applications, and limitations of S-shaped curves, also known as logistic S-curves.
Key Points:


Objective: The paper aims to provide a comprehensive understanding of the application of S-shaped curves in various domains, including technology, economics, and social sciences (Page 2).


Origins and History: The logistic S-curve originated as a model for population growth and has been adapted for various other applications, including technology forecasting, market penetration analyses, and ecological modeling (Page 2-3).


Limiting Resources: The paper argues that the power of S-curves in forecasting comes from the concept of limiting resources, which is fundamental to any growth process (Page 3).


Applications: S-curves have been used in a wide range of fields, from economics and technology to ecology and social sciences. They are often used for analyzing past data to identify new trends and validate known ones (Page 3-4).


TRIZ and S-Curves: In the framework of TRIZ (Theory of Inventive Problem Solving), S-curves are used for the qualitative study of technical systems evolution. The paper discusses the concept of multi-level resource limitations to improve the reliability of forecasts for emerging technologies (Page 4-5).


Relevance to Your Research Question:
Your research question focuses on the use of integrated machine learning models for forecasting margins and risks in construction projects, particularly utilizing deviations from S-curves. This paper is highly relevant for several reasons:


S-Curve Theory: The paper provides a deep understanding of S-curve theory, which is directly related to your research on deviations from S-curves.


Forecasting Power: Understanding the power of S-curves in forecasting due to limiting resources can add depth to your research, especially in risk assessment and actionable recommendations.


Resource Limitations: The concept of multi-level resource limitations discussed in the paper could be integrated into your machine learning model to make it more robust and reliable.


Methodological Insights: The paper's discussion on the application of S-curves in various fields can offer methodological insights that could be beneficial for your research.


TRIZ Framework: The paper's discussion on the TRIZ framework and its application of S-curves for technical systems evolution could offer another layer of complexity to your integrated approach.


},
	file = {Submitted Version:/Users/tobiasbrammer/Zotero/storage/KP67QHYC/Kucharavy and De Guio - 2011 - Application of S-shaped curves.pdf:application/pdf},
}

