@article{luu_quantifying_2009,
	title = {Quantifying schedule risk in construction projects using {Bayesian} belief networks},
	volume = {27},
	issn = {02637863},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0263786308000380},
	doi = {10.1016/j.ijproman.2008.03.003},
	abstract = {Delays on construction projects cause financial losses for project stakeholders in developing countries. This paper describes how Bayesian belief network (BBN) is applied to quantify the probability of construction project delays in a developing country. Sixteen factors were identified through a questionnaire survey of 166 professionals. Eighteen cause-effect relationships among these factors were obtained through expert interview survey to develop a belief network model. The validity of the proposed model is tested using two realistic case studies. The findings of the study revealed that financial difficulties of owners and contractors, contractor’s inadequate experience, and shortage of materials are the main causes of delay on construction projects in Vietnam. The results encourage practitioners to benefit from the BBNs. This approach is general and, as such, it may be applied to other construction projects with minor modifications.},
	language = {en},
	number = {1},
	urldate = {2023-09-15},
	journal = {International Journal of Project Management},
	author = {Luu, Van Truong and Kim, Soo-Yong and Tuan, Nguyen Van and Ogunlana, Stephen O.},
	month = jan,
	year = {2009},
	pages = {39--50},
	annote = {The paper titled "Quantifying schedule risk in construction projects using Bayesian belief networks" by Van Truong Luu et al. focuses on the application of Bayesian Belief Networks (BBN) to quantify the probability of construction project delays in developing countries, specifically Vietnam. The paper is structured into several key sections:


Introduction: The paper starts by emphasizing the importance of schedules in construction projects and how they often contain significant uncertainty. It also highlights the risky construction environment in Vietnam due to factors like poor infrastructure and bureaucratic government.


Literature Review: The authors review previous studies on significant factors causing delays in construction projects and introduce Bayesian Belief Networks (BBNs) as a tool for modeling cause-effect relationships among variables.


Research Methodology: The study adopts a two-phase approach: qualitative and quantitative analysis. In the qualitative phase, 42 factors causing delays were identified through expert interviews. These were narrowed down to 16 significant factors through a questionnaire survey. In the quantitative phase, cause-and-effect relationships among these 16 factors were determined to develop a BBN model.


Findings: The study identifies financial difficulties of owners and contractors, contractor's inadequate experience, and shortage of materials as the main causes of delay in construction projects in Vietnam.


Conclusion: The paper concludes that the BBN approach is general and can be applied to other construction projects with minor modifications.


Key Quotes and Page Citations:


"Delays on construction projects cause financial losses for project stakeholders in developing countries." (Page 1)


"Sixteen factors were identified through a questionnaire survey of 166 professionals." (Page 1)


"The study considered BBNs as the tool for predicting the probability of schedule delay because BBNs have the following principal advantages..." (Page 3)


"Sixteen significant causes of construction delay in Vietnam construction industry (VCI)" (Page 4)


"Eighteen cause-and-effect relationships among variables" (Page 5)


},
