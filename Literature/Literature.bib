@article{khodakarami_project_2014,
	title = {Project cost risk analysis: {A} {Bayesian} networks approach for modeling dependencies between cost items},
	volume = {32},
	doi = {10.1016/j.ijproman.2014.01.001},
	abstract = {Uncertainty of cost items is an important aspect of complex projects. Cost uncertainty analysis aims to help decision makers to understand and model different factors affecting funding exposure and ultimately estimate the cost of project. The common practice in cost uncertainty analysis includes breaking the project into cost items and probabilistically capturing the uncertainty of each item. Dependencies between these items are important and if not considered properly may influence the accuracy of cost estimation. However these dependencies are seldom examined and there are theoretical and practical obstacles in modeling them.  This paper proposes a quantitative assessment framework integrating the inference process of Bayesian networks (BN) to the traditional probabilistic risk analysis. BNs provide a framework for presenting causal relationships and enable probabilistic inference among a set of variables. The new approach explicitly quantifies uncertainty in project cost and also provides an appropriate method for modeling complex relationships in a project, such as common causal factors, formal use of experts' judgments, and learning from data to update previous beliefs and probabilities. The capabilities of the proposed approach are explained by a simple example.},
	number = {7},
	journal = {International Journal of Project Management},
	author = {Khodakarami, Vahid and Abdi, Abdollah},
	month = oct,
	year = {2014},
	doi = {10.1016/j.ijproman.2014.01.001},
	note = {MAG ID: 2042794667},
	pages = {1233--1245},
	annote = {The paper titled "Project cost risk analysis: A Bayesian networks approach for modeling dependencies between cost items" by Vahid Khodakarami and Abdollah Abdi focuses on addressing the uncertainty of cost items in complex projects. The paper proposes a quantitative assessment framework that integrates Bayesian networks (BN) with traditional probabilistic risk analysis to model dependencies between cost items.
Key Points:


Objective: The paper aims to improve the assessment of the likelihood distribution of project costs by explicitly quantifying uncertainty and modeling complex relationships between cost items (Page 1).


Methodology: The paper employs Bayesian networks to model dependencies between cost items. Bayesian networks provide a framework for presenting causal relationships and enable probabilistic inference among a set of variables (Page 2).


Challenges in Existing Methods: Traditional methods like Monte Carlo simulations and Regression modeling do not adequately capture the dependencies between project cost items. The paper argues that ignoring these dependencies can lead to inaccurate cost estimations (Page 2-3).


Bayesian Networks: The paper proposes using Bayesian networks to model the uncertainty of common characteristics and performance indicators affecting cost items. This approach aims to capture the causal relationships between uncertainty sources and cost items, which are often not modeled in current techniques (Page 2).


Comparison with Other Techniques: The paper reviews several other techniques used for project cost estimating, such as Regression modeling, Artificial Neural Networks (ANNs), and Monte Carlo simulations, and discusses their limitations (Page 3).


